<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.KIP.kip">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/kip_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/kip_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.KHPI_in_phone"
        android:usesCleartextTraffic="true">
        <activity android:name="com.KIP.kip.self_info_page"/>
        <activity android:name="com.KIP.kip.person_info_page" />
        <activity android:name="com.KIP.kip.noAuth_page" />
        <activity android:name="com.KIP.kip.record_book_page" />
        <activity android:name="com.KIP.kip.select_AuditoryProff_page" />
        <activity android:name="com.KIP.kip.select_BuidlingFaculty_page" />
        <activity android:name="com.KIP.kip.schedule_by_day_page" />
        <activity android:name="com.KIP.kip.select_day_schedule_page" />
        <activity android:name="com.KIP.kip.MainScreen_page" />
        <activity android:name="com.KIP.kip.log_in_page" />
        <activity android:name="com.KIP.kip.Enter_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

    </application>

</manifest>